<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <context:component-scan base-package="blockChainService" />
    <dubbo:application name="blockChain-provider" owner="blockChainGroup" organization="buaa"/>
    <dubbo:registry address="zookeeper://127.0.0.1:3002?backup=127.0.0.1:3001,127.0.0.1:3003"/>
    <dubbo:protocol name="blockChain" port="20881" />
    <dubbo:monitor protocol="registry" />
    <dubbo:service interface="blockChainService.api.BlockChainService" ref="blockChainService"/>


